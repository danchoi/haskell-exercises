cabal-version:       >=1.10

executable monadplus
  main-is:             monadplus.hs
  build-depends:       base >=4.6 && <4.7
  default-language:    Haskell2010
executable writer
  main-is:             writer.hs
  build-depends:       base >=4.6 && <4.7, mtl
  default-language:    Haskell2010
executable reader
  main-is:             reader.hs
  build-depends:       base >=4.6 && <4.7, mtl
  default-language:    Haskell2010

executable blazereader
  main-is: blazereader.hs
  build-depends:       
      base >=4.6 && <4.7,
      mtl,
      blaze-html,
      blaze-markup,
      text,
      bytestring
  default-language:    Haskell2010

executable blazesimple
  main-is: blazesimple.hs
  build-depends:       
      base >=4.6 && <4.7,
      blaze-html,
      blaze-markup,
      text,
      bytestring
  default-language:    Haskell2010

executable blazelayout
  main-is: blazelayout.hs
  build-depends:
      base >= 4.6 && < 4.7,
      blaze-html,
      blaze-markup,
      text, 
      bytestring
  default-language:   Haskell2010

executable monadsupply
  main-is: monadsupply.hs
  build-depends:
      base >= 4.6 && < 4.7,
      mtl,
      random
  default-language:   Haskell2010

executable async
  main-is: async.hs
  build-depends:
      base >= 4.6 && < 4.7,
      mtl,
      async,
      curl >= 1.3.8
  default-language:   Haskell2010

executable attoparsec
  main-is: attoparsec.hs
  build-depends:
      base >= 4.6 && < 4.7,
      attoparsec
  default-language:   Haskell2010


